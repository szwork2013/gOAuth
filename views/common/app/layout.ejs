<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="HandheldFriendly" content="True" />
    <meta name="MobileOptimized" content="640" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />
    <meta http-equiv="cleartype" content="on" />
    <meta name="screen-orientation" content="portrait">
    <meta name="x5-orientation" content="portrait">
    <meta content="telephone =no" name="format-detection" />
    <script>
        var winWidth = 0, winHeight = 0;
        if((navigator.userAgent.match(/iPhone/i) || navigator.userAgent.match(/iPod/i))) {
            // 判断系统版本号是否大于 4
           if (Boolean(navigator.userAgent.match(/OS [5-9]_\d[_\d]* like Mac OS X/i))) {
                sessionStorage.winWidth = screen.width;
                sessionStorage.winHeight = screen.height;
            }
        }
        if (sessionStorage.winWidth) {
            winWidth = sessionStorage.winWidth;
        } else {
            if (window.innerWidth)
                winWidth = window.innerWidth;
            else if ((document.body) && (document.body.clientWidth))
                winWidth = document.body.clientWidth;
            sessionStorage.winWidth = winWidth;
        }
        var scale = winWidth / 640;
        if (sessionStorage.winHeight) {
            winHeight = sessionStorage.winHeight;
        } else {
            if (window.innerHeight)
                winHeight = window.innerHeight;
            else if ((document.body) && (document.body.clientHeight))
                winHeight = document.body.clientHeight;
            sessionStorage.winHeight = winHeight;
        }
        document.write('<meta id="viewport" content="width=device-width, initial-scale=' + scale + ', maximum-scale=' + scale + ', minimum-scale=' + scale + ', user-scalable=no" name="viewport">');
    </script>
    <link rel="shortcut icon" type="image/x-icon" href="../img/logo.ico" media="screen" />
    <title><%=title%></title>
    <!-- 字体图标 -->
    <link href="../css/font-awesome.min.css" rel="stylesheet" />
    <!-- 简单特效 -->
    <link href="../css/animate.min.css" rel="stylesheet" />
    <!-- 全局样式 -->
    <link href="../css/style.css" rel="stylesheet">
    <!-- 微信jdk -->
    <script src="http://res.wx.qq.com/open/js/jweixin-1.0.0.js"></script>
    <!-- jquery -->
    <script src="../js/jquery.min.js"></script>
    <script type="text/javascript">
    </script>
</head>
<body>
    <!-- 加载层 -->
    <div class="loading-container">
        <div class="loader"></div>
    </div>
    <!-- 弹出层 -->
    <div class="app_alert">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-title"></div>
                <div class="modal-body">
                </div>
                <div class="modal-footer">
                    <button class="btn confirm" style="display: none;" onclick="$(this).hide();$('body').css('overflow', 'auto');$('.app_alert').hide();if (typeof confirmFun != 'undefined' && confirmFun instanceof Function) confirmFun();">确认</button>
                    <button id="btnGb" class="btn" onclick="$('body').css('overflow', 'auto');$('.app_alert').hide();">取消</button>
                </div>
            </div>
        </div>
    </div>
    <!-- 主页面 -->
    <div class="app" style="display:none;">
        <%-include(pagePath, pageJson)-%>
    </div>
    <!-- 菜单页 -->
    <% if (isShowMenu) { %>
        <%-include("./layout_menu", pageJson)-%>
    <% } %>
    <!-- 选择层 -->
    <!-- <div class="choose_mask"></div> -->
</body>
<!-- 参数 -->
<input id="appid" type="hidden" value="<%= appId || '' %>"/>
<input id="title" type="hidden" value="<%= title || '' %>"/>
<!-- 验证 -->
<script src="../js/app/validator.js"></script>
<!-- 公共js -->
<script src="../js/app/common.js"></script>
</html>