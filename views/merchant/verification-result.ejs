	<% include header.ejs %>
	<title>核销卡券</title>
	<%if(isDebug){%>
		<link rel="stylesheet" href="/merchant/css/common.css" />
		<link rel="stylesheet" href="/merchant/css/verification.css" />
	<%}else{%>
		<link rel="stylesheet" href="/merchant/css/common.css" />
		<link rel="stylesheet" href="/merchant/css/verification.css" />
	<%}%>
</head>
<body>
	<ul class="top-menu">
		<li class="selected"><a href="/merchant/verificationSn">核销卡券</a><div class="underline"></div></li>
		<li><a href="/merchant/verificationRecord">核销记录</a></li>
		<li><a href="/merchant/settLog">结算历史</a></li>
	</ul>
	<div class="hr"></div>
	<div class="step">
		<div class="circle selected">1</div>
		<div class="line selected"></div>
		<div class="circle selected">2</div>
		<div class="line selected"></div>
		<div class="circle selected">3</div>
	</div>
	<ul class="step-words">
		<li class="selected">券号输入/扫一扫</li>
		<li class="selected">核对卡券信息</li>
		<li class="selected">核销完成</li>
	</ul>
	<div class="hr"></div>
	<div class="success">
		<img style="width:88px; height:88px" src="/merchant/img/success.png"/>
		<div style="font-size:3rem; color:#009b31; margin-top:22px"><%=errmsg%>!</div>
		<div style="color:#666; font-size:2.2rem; width:22.2rem; margin:16px auto 0;">可在"核销记录"中查看核销历史记录。</div>
		<div style="margin-top:54px; height:65px; font-size:2.7rem;">
			<div class="confirm" style="float:left">返回继续核销</div>
			<div class="cancel" style="color:white; float:right">关闭</div>
		</div>
	</div>
	<input type="hidden" name="appId" value="<%=appId%>"/>
	<script src="/js/prefixfree.min.js"></script>
	<script src="/js/jquery.min.js"></script>
	<%if(isDebug){%>
		<script src="/merchant/js/common.js"></script>
	<%}else{%>
		<script src="/merchant/js/common.min.js"></script>
	<%}%>
	<script>
		$('.cancel').tap(function(){
			
		});
		$('.confirm').tap(function(){
			location.href='/merchant/verificationSn';
		});
	</script>
</body>
</html>